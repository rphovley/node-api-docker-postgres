{
  "name": "red-panda",
  "version": "1.0.0",
  "description": "",
  "main": "start.js",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:350labs/redpanda/api.git"
  },
  "scripts": {
    "start": "bash scripts/start-api.sh",
    "start-db": "bash scripts/start-db.sh",
    "test": "ts-mocha --paths -p server/src/tsconfig.json test/**/*.test.ts",
    "logs": "docker-compose -p red-panda -f docker-compose.dev.yml logs -f",
    "kill": "docker-compose -p red-panda -f docker-compose.dev.yml kill",
    "connect": "docker exec -it red-panda_success_db psql -U red-panda",
    "connect-global": "docker exec -it red-panda_global_db psql -U red-panda",
    "migrate": "bash ./knex/migrate_and_seed.sh",
    "migrate-global": "cd ./knex/global && ts-node migrate.ts",
    "migrate-tenant": "cd ./knex/multi-tenant && ts-node migrate.ts",
    "seed-global": "cd ./knex/global && ts-node seeds.ts",
    "seed-tenant": "cd ./knex/multi-tenant && ts-node seeds.ts",
    "compile": "gulp compilejs",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@google-cloud/logging-bunyan": "^2.0.2",
    "body-parser": "^1.19.0",
    "bookshelf": "^1.0.1",
    "bunyan": "^1.8.12",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.9.0",
    "knex": "^0.19.5",
    "morgan": "^1.9.1",
    "objection": "^1.6.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "prettier": "^1.19.1"
  },
  "devDependencies": {
    "@types/bookshelf": "^0.13.3",
    "@types/bunyan": "^1.8.6",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.1",
    "@types/knex": "^0.16.1",
    "@types/mocha": "^5.2.7",
    "@types/passport": "^1.0.1",
    "@types/passport-jwt": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "faker": "^4.1.0",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-nodemon": "^2.4.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "mocha": "^6.2.1",
    "prettier-eslint": "^9.0.1",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.6.1",
    "typescript": "^3.6.4"
  }
}
