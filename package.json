{
  "name": "template",
  "version": "0.0.1",
  "description": "",
  "main": "server/dist/server.js",
  "repository": {
    "type": "git",
    "url": "template"
  },
  "scripts": {
    "start": "node server/dist/server.js",
    "dev": "bash scripts/start-api.sh",
    "start-db": "bash scripts/start-db.sh",
    "compile": "gulp compilejs",
    "gcp-build": "yarn compile",
    "test": "ts-mocha --paths -p server/src/tsconfig.json test/**/*.test.ts",
    "logs": "docker-compose -p template -f docker-compose.dev.yml logs -f",
    "kill": "docker-compose -p template -f docker-compose.dev.yml kill",
    "connect": "docker exec -it template_success_db psql -U template",
    "connect-global": "docker exec -it template_global_db psql -U template",
    "generate-token": "ts-node --project ./server/src/tsconfig.json ./scripts/generate_user_token.ts",
    "generate-token-new": "ts-node --project ./server/src/tsconfig.json ./scripts/generate_token_new_user.ts",
    "migrate": "bash ./deployment/migrate.sh",
    "seed": "bash ./deployment/migrate_and_seed.sh",
    "migrate-global": "cd ./deployment/global && ts-node migrate.ts",
    "migrate-tenant": "cd ./deployment/multi-tenant && ts-node migrate.ts",
    "seed-global": "cd ./deployment/global && ts-node seeds.ts",
    "seed-tenant": "cd ./deployment/multi-tenant && ts-node seeds.ts",
    "build-deployer": "docker build ./deployment --tag  registry.gitlab.com/rphovley-templates/node-api-express-objection-postgres/deployer",
    "push-deployer": "docker push registry.gitlab.com/rphovley-templates/node-api-express-objection-postgres/deployer",
    "test-runner": "gitlab-runner exec docker --docker-privileged --cache-dir=/tmp/cache --docker-volumes /tmp/cache:/tmp/cache $(cat ./deployment/ci.env |cut -f 2 |sed 's/^/--env /g'| xargs -0) deploy_staging",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@google-cloud/logging-bunyan": "^2.0.2",
    "body-parser": "^1.19.0",
    "bookshelf": "^1.0.1",
    "bunyan": "^1.8.12",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.9.0",
    "gae-ayaml-env": "^0.0.19",
    "knex": "^0.19.5",
    "morgan": "^1.9.1",
    "objection": "^2.1.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "prettier": "^1.19.1"
  },
  "devDependencies": {
    "@types/bookshelf": "^0.13.3",
    "@types/bunyan": "^1.8.6",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.1",
    "@types/knex": "^0.16.1",
    "@types/mocha": "^5.2.7",
    "@types/passport": "^1.0.1",
    "@types/passport-jwt": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-engage-api": "./eslint",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "faker": "^4.1.0",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-nodemon": "^2.4.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.1",
    "mocha": "^6.2.1",
    "prettier-eslint": "^9.0.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.6.1",
    "typescript": "^3.6.4"
  }
}
