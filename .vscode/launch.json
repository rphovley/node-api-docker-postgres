  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Docker",
      // "preLaunchTask": "tsc-watch",
      "restart": true,
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/f0-datalake_api",
      "sourceMaps": true,
      "address": "0.0.0.0",
      "port": 9222,
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js",
      ]
    }
  ]
}